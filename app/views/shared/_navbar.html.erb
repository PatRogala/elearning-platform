<% nav_items = [
  {
    name: "FAQ",
    path: faq_path
  }
]
%>

<% if current_user&.admin? %>
  <%= link_to "Admin", admin_path, class: "font-medium text-gray-500 hover:text-gray-400 sm:py-6 dark:text-gray-400 dark:hover:text-gray-500" %>
<% end %>

<% nav_items.each do |item| %>
  <a href="<%= item[:path] %>" class="font-medium text-gray-500 hover:text-gray-400 sm:py-6 dark:text-gray-400 dark:hover:text-gray-500"><%= item[:name] %></a>
<% end %>
